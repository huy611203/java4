<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="https://jakarta.ee/xml/ns/persistence/orm"
                 version="3.0">

    <entity class="com.example.demo2.model.VideoEntity">
        <table name="Video" schema="dbo" catalog="Java4"/>
        <attributes>
            <id name="id">
                <column name="id" length="10"/>
            </id>
            <basic name="title">
                <column name="title" nullable="false" length="100"/>
            </basic>
            <basic name="url">
                <column name="url" nullable="false" length="100"/>
            </basic>
            <basic name="views">
                <column name="views" nullable="false" precision="-1"/>
            </basic>
            <basic name="shares">
                <column name="shares" nullable="false" precision="-1"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
